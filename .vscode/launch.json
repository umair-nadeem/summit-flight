    "version": "0.2.0",
    "configurations": [
        {
            "name": "aeromight",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/out/build/stm_m4/artifacts/aeromight",
            "request": "launch",
            "type": "cortex-debug",
            "showDevDebugOutput": "raw",
            "device": "STM32F411CEU6",
            "interface": "swd",
            "servertype": "openocd",
            "runToEntryPoint": "main",
            "svdFile": "${workspaceFolder}/external/stm32f411/STM32F411.svd",
            "configFiles": [
            "interface/stlink.cfg",
            "target/stm32f4x.cfg"
            ],
            "preLaunchCommands": [
            "monitor reset halt"
            ],
            "postRestartCommands": [
            "load"
            ]
        },
        {
            "name": "LoggingTest",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/native/common/all/logging/test/logging_test",
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false
        },
        {
            "name": "Mpu6500Test",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/out/build/native/common/all/mpu6500/test/mpu6500_test",
            "args": [],
            "cwd": "${workspaceFolder}",
            "env": {},
            "stopOnEntry": false
        }
      ]
}
